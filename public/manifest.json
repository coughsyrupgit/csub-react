{
  "name": "CS Useful Bookmarks",
  "description": "CS Useful Bookmarks React Version",
  "version" : "0.3.0",
  "icons": {
      "16": "logo_16.png",
      "48": "logo_48.png",
      "128": "logo_128.png"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": [
    "tabs",
    "bookmarks",
    "storage",
    "unlimitedStorage",
    "chrome://favicon/"
  ],
  "content_security_policy": "script-src 'self'; object-src 'self'",
  "manifest_version": 2
}
