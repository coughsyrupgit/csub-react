{
  "name": "CS Useful Bookmarks",
  "description": "CS Useful Bookmarks React Version",
  "version": "0.2.0",
  "options_page" : "configure.html",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "browser_action": {
    //"default_icon": "icon.png",
    //"default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "bookmarks",
    "storage",
    "unlimitedStorage",
    "chrome://favicon/"
  ],
  "content_security_policy": "script-src 'self'; object-src 'self'",
  "manifest_version": 2
}